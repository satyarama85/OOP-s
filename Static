class parent;
  int a;
  int b;
  function void display();
    $display("a=%0d,b=%0d",a,b);
  endfunction
endclass
class child extends parent;
  int c=20;
  function void display();
    $display("c=%0d",c);
  endfunction
endclass
module tb;
  child c1;
  initial begin
    c1=new();
    c1.display();
  end
endmodule

